public class Task_4 {

	public static void main(String[] args) {
		int[] arr = { 2, 2, 3, 3, 3, 2, 6, 1, 4, 1 };

		int counter = 1;
		int counterTemp = 1;
		
			
			
			for (int j = 1; j < arr.length; j++) {
				if (arr[j] == arr[j - 1]) {
					counterTemp++;
				} 
				if (counterTemp>counter) {
					counter=counterTemp;
					
				}
				if (!(arr[j] == arr[j - 1])) {
					counterTemp=1;
				} 
			}
			
			
		
		
		System.out.println(counter);

	}

}
