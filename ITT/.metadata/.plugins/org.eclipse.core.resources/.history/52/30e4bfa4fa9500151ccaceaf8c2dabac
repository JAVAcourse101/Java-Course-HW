public class Task_10 {

	public static void main(String[] args) {
		int day = 12;
		int month = 11;
		int year = 1999;

		int daysInMonth = 31;

		boolean leap = ((((year % 100 != 0) && year % 4 == 0)) || (year % 400 == 0));

		day += 1;
		System.out.println(day + " " + month + " " + year + " ");

		switch (month) {
		case (2):
			if (leap) {
				daysInMonth = 29;
			} else {
				daysInMonth = 28;
			}
			break;
		case (4):

		case (6):

		case (9):

		case (11):
			daysInMonth = 31;
			break;
		}

	}

}
