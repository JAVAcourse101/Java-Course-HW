import java.util.Scanner;


public class Task_3 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println(Long.MAX_VALUE);
		System.out.println("Please enter a three digit number");
		long num=sc.nextInt();
		
		do{

			System.out.print(num%10);
			num /=10;
				
		} while (num>0);
		sc.close();

	}

}
