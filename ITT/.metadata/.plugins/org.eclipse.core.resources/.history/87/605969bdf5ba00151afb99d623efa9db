package task_1;

public interface INotepad {
	
	
	void addTextToPage(int pageNum);
	void replaceTextInPage(String newText,int pageNum);
	void deleteTextInPage(int pageNum);
	void printAllPages();
	void searchWord(String word);
	void printAllPagesWithdigits();
	
	static final int DEFAULT_NUMBER_OF_PAGES = 50;



}
