import java.util.Scanner;


public class Five {

	public static void main(String[] args) {
		System.out.println("Please enter three numbers and we will print them in descending order.");
		Scanner test= new Scanner(System.in);
		
		//I know that I had to use only if statements, but this was easier for me and I can explain how it works step by step!!!
		
		int[] Store = new int[3];
		System.out.println("Please enter the first one:");
		Store[0] =test.nextInt();
		System.out.println("Please enter the second one:");
		Store[1] = test.nextInt();
		System.out.println("Please enter the third one:");
		Store[2] = test.nextInt();
	
		
		for (int i=0;i<2;i++){
			
			for (int h=0;h<2;h++){
				
				if(Store[h]<Store[h+1]){
					
				Store[h] = Store[h]+Store[h+1];
		        Store[h+1] = Store[h]-Store[h+1];
		        Store[h] = Store[h]-Store[h+1];
				}
			}
			
			
		}
		
		for (int i=0;i<=2;i++){
			System.out.print(Store[i]+", ");
			
		}
		test.close();

	}

}
