import java.util.Scanner;

public class Task_3 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Please enter number");
		int num = sc.nextInt();
		int numTemp = num;
		int numRev = 0;

		int digits = 0;

		for (; numTemp > 0; digits++) {
			numTemp /= 10;

		}
		
		System.out.println(digits);

	}
}
