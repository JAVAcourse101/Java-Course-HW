public class Task_11 {

	public static void main(String[] args) {
		int[] array = {  };
		printArr(array);
	}

	static void printArr(int arr[]) {

		if (arr.length == 0) {
			System.out.println("You have entered an empty array");
			return;
		}
		for (int i = 0; i < arr.length; i++) {
			System.out.print(arr[i] + " ");
		}
	}
}
