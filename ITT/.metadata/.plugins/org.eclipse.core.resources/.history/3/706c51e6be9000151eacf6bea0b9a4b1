public class Task_4 {

	public static void main(String[] args) {

		int a = 3;

		int b = 4;

		int c = 4;

		int d = 3;

		int e = 5;

		int tempA = ((a ^ b) * (a ^ c) * (a ^ d) * (a ^ e))&1;
		int tempB = ((a ^ b) * (b ^ c) * (b ^ d) * (b ^ e))&1;
		int tempC = ((c ^ b) * (a ^ c) * (c ^ d) * (c ^ e))&1;
		int tempD = ((d ^ b) * (d ^ c) * (a ^ d) * (d ^ e))&1;
		int tempE = ((e ^ b) * (e ^ c) * (e ^ d) * (e ^ a))&1;

	
		int uni = a * tempA + b * tempB+ c * tempC+ d * tempD+ e * tempE;

		 System.out.println(uni);
	}
}
