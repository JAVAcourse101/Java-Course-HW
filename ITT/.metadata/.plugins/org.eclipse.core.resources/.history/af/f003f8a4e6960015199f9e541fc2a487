public class Task_5 {

	public static void main(String[] args) {
		int[] arr = { 3, 3, 2, 3, 3, 3, 2, 1, 1, 1, 1 };
		double avverage = 0;
		double sum = 0;
		for (int i = 0; i < arr.length; i++) {
			sum += arr[i];
		}
		avverage=sum/arr.length;
		
		System.out.println("The avverage is "+avverage);
	}

}
