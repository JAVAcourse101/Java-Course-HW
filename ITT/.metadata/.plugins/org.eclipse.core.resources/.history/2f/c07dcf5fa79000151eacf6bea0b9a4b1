import java.util.Scanner;


public class Task_3 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Please enter a three digit number");
		int num=sc.nextInt();
		
		System.out.print(num%10);
		num /=10;
		System.out.print(num%10);
		num /=10;
		System.out.print(num%10);
		num /=10;

	}

}
