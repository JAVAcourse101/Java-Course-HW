public class Task_9 {

	public static void main(String[] args) {
		long x = 1310725;
		long mark = 131072;
		
		
		long z=((x-mark)>>63)&1;
		
//		System.out.println(z);
		long bigger=x -z*(x-mark);
		
		boolean res=(bigger==x);
		
		System.out.println(res);
		// boolean test = (mark == 1);
		
		
		
	}
}
