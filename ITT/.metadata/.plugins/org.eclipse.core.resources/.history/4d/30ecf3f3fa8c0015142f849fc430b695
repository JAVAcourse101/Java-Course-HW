public class Task_11_2 {

	public static void main(String[] args) {

		int row = 4;

		int col = 5;
		int[][] arr = new int[row][col];

		int num = 1;
		
		

		for (int j = 0; j < col; j++) {

			int r = 0;
			int c = j;
			
			while (r < row) {
				arr[r][c] = num;
				num++;
				c++;
			}

		}

		for (int j = 0; j < col; j++) {
			for (int i = 0; i < row; i++) {
				
				
				System.out.print(arr[i][j] + " ");
			}
			System.out.println();

		}

	}

}
