import java.util.Scanner;

public class Task_4 {

	public static void main(String[] args) {
		System.out
				.println("Please enter two names delimited by comma.");
		Scanner sc = new Scanner(System.in, "ASCII");

		System.out.println("Please enter the first word");
		String str = sc.nextLine();
		str = str.trim();
		
		int indexCom=0;
		
		for (int i = 0; i < str.length(); i++) {
			if (str.charAt(i) == ' ') {
				indexCom = i;
				break;
			}
		}
		
		char[] arr = str.toCharArray();
		char[] nameOne = new char[indexCom + 1];
		char[] nameTwo = new char[str.length() - indexCom - 1];

		for (int i = 0; i < arr.length; i++) {
			if (i <= indexCom) {
				nameOne[i] = arr[i];
			}

			if (i > indexCom) {
				nameTwo[i - indexCom - 1] = arr[i];
			}
		}
		
		String nameO = new String(nameOne);
		String nameT = new String(nameTwo);
		nameT=nameT.trim();
		
		int sum=0;
		for(int i=0;i<nameO.length();i++){
			sum +=nameO.charAt(i);
					
		}
		System.out.println(sum);
	}

}
