
public class Task_03 {

	public static void main(String[] args) {
		int[] arr = { 3,2, 3, 4, 5, 6, 13, 8, 9,10, 16 };
		System.out.println(isIncr(arr, arr.length-1));
	}

	static boolean isIncr(int[] arr,int num) {
		
		if (num == 1) {
			if (arr[0]<arr[1]) {
				return true;
			}
			else{
				return false;
			}

		}
		
		
		return isIncr(arr,num-1);
		
	}

}
