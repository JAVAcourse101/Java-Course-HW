
public class Task_11_2 {

	public static void main(String[] args) {
		
		int r=4;
		
        int c=5;
		int[][] arr = new int[r][c];

		int num = 1;

		for (int j = 0; j < c; j++) {
			for (int i = 0; i < r; i++) {
				arr[i][j] = num;
				num++;
				System.out.print(arr[i][j] + " ");
			}
			System.out.println();

		}

	}

}
