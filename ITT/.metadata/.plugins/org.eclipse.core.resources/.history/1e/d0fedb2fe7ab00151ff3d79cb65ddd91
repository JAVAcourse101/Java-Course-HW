package Task_01;

public class Person {

	private String name;
	private byte Age;
	private boolean isMale;

	Person(String name, byte age, boolean isMale) {

	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		if (name == null || name.length() == 0) {
			return;

		}
		this.name = name;
	}

	public byte getAge() {
		return Age;
	}

	public void setAge(byte age) {
		if (age<0) {
			return;
		}
		Age = age;
	}

	public boolean isMale() {
		return isMale;
	}

}
