import java.util.Scanner;


public class expression {

	public static void main(String[] args) {
//		Scanner input = new Scanner(System.in,"UTF-8");
//		System.out.println("Please an expression like this 9+6+3+4+8+6-7-9-5+5+7"); 
		String ex="1++3+4+8+6-7-9-5+5+7";
		int len=ex.length();
		
		for(int i =0;i<len;i++){
			if (i%2==0){
				
			}
			if (i%2!=0){
				if(Character.isDigit(ex.charAt(i))==false){
					int miss=i+1;
					System.out.println("Your expression has a misstake, symbol number "+miss+"must be a digit between 0-9");
				}
				
			}
			if (i==0){
				if(Character.isDigit(ex.charAt(i))==false){
					int miss=i+1;
					System.out.println("Your expression has a misstake, symbol number "+miss+"must be a digit between 0-9");
				}
				
			}
			
		}
		
				

	}

}
