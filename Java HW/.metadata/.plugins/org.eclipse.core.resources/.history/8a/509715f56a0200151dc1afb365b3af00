import java.util.Random;




public class Wow{

	public static void main(String[] args) {
		
		Random ran = new Random();
		int top = 5;
		char data = ' ';
		String dat = "";

		for (int i=0; i<=top; i++) {
		  data = (char)(ran.nextInt(25)+97);
		  dat = data + dat;
		}

		greet(dat);
		

	}
	
	
	
	public static void greet(String name){
		System.out.println("Greeting, " +name+" !");
	}
	
}

