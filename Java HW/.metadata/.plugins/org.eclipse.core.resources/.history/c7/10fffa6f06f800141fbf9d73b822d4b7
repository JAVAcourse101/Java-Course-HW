import java.util.Scanner;


public class SearchInArrey {

	public static void main(String[] args) {
    Scanner input = new Scanner(System.in,"UTF-8");
		
		System.out.println("Hello " );
		System.out.println("We have this arrey please anter a number and we will tell you the index of that number if it is in the arrey " );
		int L = 18;
		
		int[] n = new int[L];

		for (int i = 0; i < L; i++) {
			n[i] = i + 1;
		    System.out.print( n[i]+ "|" );
		    
		}
		input.close();

	}

}
