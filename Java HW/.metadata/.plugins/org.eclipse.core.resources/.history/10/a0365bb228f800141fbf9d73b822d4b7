import java.util.Scanner;


public class VerticalHorizontal {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in,"UTF-8");
		System.out.println("Please enter the size of the square two dimencional arrey " );
 		int L = input.nextInt();
 		
 		int [][] m = new int [L][L]; 
		for(int row = 0; row < m.length; row++){
			for(int col = 0; col < m[row].length; col++){
				System.out.print(m[row][col] + " ");
		}
			System.out.println();
			
			for(int row1 = 0; row1 < m.length; row1++){
				for(int col = 0; col < m[row1].length; col++){
					m[row1][col]=1+row1*L;
					
					System.out.print(m[row1][col] + " ");
				}
				System.out.println();
			}		
		}

	}

}
