import java.util.Scanner;


public class five {

	public static void main(String[] args) {
		Scanner sc=new Scanner (System.in);
		System.out.println("Please enter an array of whole numbers, but for doing that first tell me how long do you want it to be");
		int l = sc.nextInt();
		
		int [] arr=new int [l];
		System.out.println("Now let us start enetering the array");
		
		for (int i = 0;i<l;i++){
			arr[i]=sc.nextInt();
			
			
		}
		for (int j = 0;j<l;j++){
			System.out.print(arr[j]+",");
			
			
		}
		
	}

}
