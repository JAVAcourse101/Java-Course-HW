import java.util.Scanner;

public class Eight {

	public static void main(String[] args) {
		boolean flag = true;

		while (flag == true) {
			Scanner input = new Scanner(System.in);
			System.out
					.println("Please enter a whole number between in the range [1000;9999]");
			int num = input.nextInt();

			if ((num < 1000) || (num > 9999)) {
				System.out.println("Your number is out of range");
				System.out.println();
				continue;
			}

			int[] digits = new int[4];
			int i = 0;
			while (num > 0) {
				digits[3 - i] = num % 10;
				i++;
				num = num / 10;

			}

			int A = digits[0] * 10 + digits[3];
			int B = digits[1] * 10 + digits[2];
			if (A > B) {
				System.out.println("The first number:" + A
						+ " is bigger than the second number:" + B);
			}
			if (A < B) {
				System.out.println("The first number:" + A
						+ " is smaller than the second number:" + B);
			}
			if (A == B) {
				System.out.println("The first number:" + A
						+ " is equal to the second number:" + B);
			}
			input.close();
			flag = false;
		}

	}

}
